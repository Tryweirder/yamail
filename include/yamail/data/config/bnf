
key = string


statement = (
    control
  | key name compound_value 
  | key value
) >> ;

value =
    compound_value
  | array 
  | string

array = '[' string... ']'

compound_value = '{' statement '}'

control = '%' >> (import | include) >> string 

string = <empty>  | ( charseq... )

charseq = chars | quoted-chars

quoted-chars = " >> chars... >> "
